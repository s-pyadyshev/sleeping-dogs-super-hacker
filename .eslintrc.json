{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "plugins": ["@typescript-eslint", "import", "react", "react-hooks"],
  "rules": {
    "react/forbid-prop-types": 0,
    "class-methods-use-this": 0,
    "function-paren-newline": 0,
    "no-confusing-arrow": 0,
    "padded-blocks": 0,
    "quote-props": 0
  }
}